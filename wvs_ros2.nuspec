<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>wvs_ros2</id>
    <version>99.99.99</version>
    <authors>wvs-support@demcon.com</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ros2 package for creating ros2 software in dotnet.</description>
    <tags>ros2 dotnet</tags>
    <dependencies>
        <group targetFramework="netstandard1.1" />
        <group targetFramework="native" />
    </dependencies>
  </metadata>
  <files>
    <file src="*_assemblies.dll" target="lib/netstandard1.1" />
    <file src="rcldotnet_common.dll" target="lib/netstandard1.1" />
    <file src="*.dll" target="lib/native/x64/Release" exclude="*_assemblies.dll;rcldotnet_common.dll"/>
  </files>
</package>